{
  "$schema": "https://turbo.build/schema.json",
  "daemon": true,
  "envMode": "strict",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "target/release/**"]
    },
    "build:linux": {
      "dependsOn": ["^build", "^build:linux"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "target/release/**"]
    },
    "build:macos": {
      "dependsOn": ["^build", "^build:macos"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "target/release/**"]
    },
    "build:windows": {
      "dependsOn": ["^build", "^build:windows"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "target/release/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
