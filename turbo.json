{
  "$schema": "https://turbo.build/schema.json",
  "daemon": true,
  "envMode": "strict",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "target/release/**", "dist/**"]
    },
    "build:prod": {
      "dependsOn": ["^build", "^build:prod"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "target/release/**", "dist/**"],
      "env": ["TARGET"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
